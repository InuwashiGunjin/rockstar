<form action="/auth/loginProccess" method="post" class="col-md-8 col-lg-3 p-2 m-auto">
    <%if(typeof data!='undefined'){%>
        <div class="h6 text-danger text-center"><%=data%></div>
    <%}%>
    <div class="m-3">
        <input type="text" class="form-control" name="username" placeholder="Unesite korisnicko ime" >
    </div>
    <div class="m-3">
        <input type="password" class="form-control" type="password" name="password" placeholder="Unesite lozinku" >
    </div>
    <div class="m-3">
        <button type="submit" class="btn btn-primary">Prijava</button>
        <br/>
        <a href="/" class="nav nav-link">Vratite se</a>
    </div>
</form>